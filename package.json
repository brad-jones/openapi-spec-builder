{
    "name": "@brad-jones/openapi-spec-builder",
    "version": "0.0.0-semantically-released",
    "description": "TypeScript Interfaces and Tools to Build OpenAPI Specs.",
    "author": "Brad Jones <brad@bjc.id.au>",
    "license": "MIT",
    "main": "./dist/OpenApiSpecBuilder.js",
    "types": "./dist/OpenApiSpecBuilder.d.ts",
    "repository": "brad-jones/openapi-spec-builder",
    "scripts": {
        "commit": "commit",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "clean": "shx rm -rf ./dist && shx rm -rf ./.nyc_output && shx rm -rf ./test-results",
        "build": "tsc -p ./src/tsconfig.json",
        "rebuild": "yarn clean && yarn build",
        "test": "tsos run ./tests/runner.ts --no-visitors",
        "cover": "nyc --exclude \"tests/**\" --report-dir ./test-results/coverage --reporter=html --reporter=clover yarn test",
        "release": "node ./scripts/release.js",
        "upgrade-packages": "ncu --upgrade --upgradeAll --packageFile ./package.json"
    },
    "dependencies": {
        "deep-equal": "^1.0.1",
        "fs-extra": "^5.0.0",
        "linq-es2015": "^2.4.31",
        "md5": "^2.2.1",
        "swagger-tools": "^0.10.3",
        "traverse": "^0.6.6",
        "tslib": "^1.9.0",
        "typescript": "^2.6.2"
    },
    "devDependencies": {
        "@brad-jones/tsos-cli": "^8.2.0",
        "@commitlint/cli": "^6.0.2",
        "@commitlint/config-angular": "^6.0.2",
        "@commitlint/prompt-cli": "^6.0.2",
        "@types/deep-equal": "^1.0.1",
        "@types/execa": "^0.8.1",
        "@types/fs-extra": "^5.0.0",
        "@types/md5": "^2.1.32",
        "@types/node": "^9.3.0",
        "@types/p-retry": "^1.0.1",
        "@types/shelljs": "^0.7.7",
        "@types/swagger-tools": "github:brad-jones/typed-swagger-tools#745508c25a520d7426156591a9287284efec1c45",
        "@types/traverse": "^0.6.30",
        "alsatian": "^2.0.0",
        "conventional-changelog": "^1.1.7",
        "conventional-recommended-bump": "^1.1.0",
        "delay": "^2.0.0",
        "execa": "^0.9.0",
        "get-stream": "^3.0.0",
        "github-release-cli": "^0.3.0",
        "glob": "^7.1.2",
        "husky": "^0.14.3",
        "listr": "^0.13.0",
        "lodash": "^4.17.4",
        "npm-check-updates": "^2.14.0",
        "nyc": "^11.4.1",
        "octonode": "^0.9.1",
        "p-retry": "^1.0.0",
        "semver": "^5.5.0",
        "shx": "^0.2.2",
        "tap-spec": "^4.1.1",
        "tap-xunit": "^2.2.0",
        "yargs": "^11.0.0"
    },
    "resolutions": {
        "@types/express-serve-static-core": "4.11.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
